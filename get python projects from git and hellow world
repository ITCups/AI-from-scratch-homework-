ab":{"base_uri":"https://localhost:8080/","height":517},"executionInfo":{"status":"error","timestamp":1619197860878,"user_tz":-420,"elapsed":27524,"user":{"displayName":"Igor Toropov","photoUrl":"","userId":"14114129747419253788"}},"outputId":"1924a95f-cd58-44b5-fd0f-11666ece4464"},"source":["from IPython.display import clear_output\n","from time import sleep\n","from google.colab import output\n","!pip install termcolor\n","import termcolor\n","# Консоль более плавная если убрать эту строку\n","output.eval_js('var myAudioElement = new Audio(\"https://media1.vocaroo.com/mp3/19OLVGThvBBg\").play()') # Музыка - Hello world : Louie Zong (1:51) - https://www.youtube.com/watch?v=Yw6u6YkTgQ4\n","\n","#Работает с разным текстом\n","text = '''\n","ooooo   ooooo           oooo  oooo                                                   oooo        .o8  \n","`888'   `888'           `888  `888                                                   `888       \"888  \n"," 888     888   .ooooo.   888   888   .ooooo.     oooo oooo    ooo  .ooooo.  oooo d8b  888   .oooo888  \n"," 888ooooo888  d88' `88b  888   888  d88' `88b     `88. `88.  .8'  d88' `88b `888\"\"8P  888  d88' `888  \n"," 888     888  888ooo888  888   888  888   888      `88..]88..8'   888   888  888      888  888   888  \n"," 888     888  888    .o  888   888  888   888       `888'`888'    888   888  888      888  888   888  \n","o888o   o888o `Y8bod8P' o888o o888o `Y8bod8P'        `8'  `8'     `Y8bod8P' d888b    o888o `Y8bod88P\" \n","'''\n","\n","colors = ('red','yellow','green','cyan','blue','magenta')\n","pickedColor = \"red\"\n","minIndent = 3\n","maxIndent = minIndent + text.count('\\n')\n","indents = [] # Лист содержащий отступы и направление в которое они движутся\n","\n","def populateIndents(): # Заполняет лист с отступами опираясь на кол-во строк в тексте\n","  global text\n","  defaultIndent = 3\n","  for i in range(text.count('\\n')+1):\n","      indents.append([defaultIndent,1])\n","      defaultIndent+=1\n","\n","def nextColor(color): # Смещает цвет\n","  global colors\n","  next = colors.index(color) + 1\n","  if next == len(colors):\n","    next = 0\n","  return colors[next]\n","\n","def shiftIndent(indent): # Смещает отступ\n","  global maxIndent; global minIndent\n","  if indent[0] == maxIndent or indent[0] == minIndent:\n","    indent[1] *= -1\n","  indent[0] += indent[1]\n","\n","def update():\n","  global pickedColor\n","  pickedColor = nextColor(pickedColor)\n","  textToPrint = \"\" # Лучше производительность в отличии от вывода каждого символа\n","  # Выводит отступ первой строки\n","  textToPrint += ' ' * indents[0][0]\n","  shiftIndent(indents[0])\n","  currentLine = 0\n","  for char in text:\n","    # Выводит отсальные отступы\n","    if (char == '\\n'):\n","      textToPrint += char\n","      currentLine+=1\n","      currentIndent = indents[currentLine]\n","      textToPrint += ' ' * currentIndent[0]\n","      shiftIndent(currentIndent)\n","      continue\n","    pickedColor = nextColor(pickedColor)\n","    textToPrint += termcolor.colored(char, pickedColor)\n","  print(textToPrint)\n","  sleep(0.5) # Скорость анимации\n","  clear_output(wait=True)\n","\n","clear_output(wait=True)\n","populateIndents()\n","while True:\n","  update()\n"],"execution_count":1,"outputs":[{"output_type":"stream","text":["\n","         \u001b[33mo\u001b[0m\u001b[32mo\u001b[0m\u001b[36mo\u001b[0m\u001b[34mo\u001b[0m\u001b[35mo\u001b[0m\u001b[31m \u001b[0m\u001b[33m \u001b[0m\u001b[32m \u001b[0m\u001b[36mo\u001b[0m\u001b[34mo\u001b[0m\u001b[35mo\u001b[0m\u001b[31mo\u001b[0m\u001b[33mo\u001b[0m\u001b[32m \u001b[0m\u001b[36m \u001b[0m\u001b[34m \u001b[0m\u001b[35m \u001b[0m\u001b[31m \u001b[0m\u001b[33m \u001b[0m\u001b[32m \u001b[0m\u001b[36m \u001b[0m\u001b[34m \u001b[0m\u001b[35m \u001b[0m\u001b[31m \u001b[0m\u001b[33mo\u001b[0m\u001b[32mo\u001b[0m\u001b[36mo\u001b[0m\u001b[34mo\u001b[0m\u001b[35m \u001b[0m\u001b[31m \u001b[0m\u001b[33mo\u001b[0m\u001b[32mo\u001b[0m\u001b[36mo\u001b